{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "client",
            "image": "vbfdt7/multi_client",
            "hostname": "client",
            "essential": false
        },
        {
            "name": "nginx",
            "image": "vbfdt7/multi_nginx",
            "hostname": "nginx",
            "essential": true
        },
        {
            "name": "worker",
            "image": "vbfdt7/multi_worker",
            "hostname": "worker",
            "essential": false
        },
        {
            "name": "server",
            "image": "vbfdt7/multi_server",
            "hostname": "api",
            "essential": false,
            "portMappings": {
                "hostPort": 80,
                "containerPost": 80
            },
            "links": ["client", "server"]
        }
    ]
}